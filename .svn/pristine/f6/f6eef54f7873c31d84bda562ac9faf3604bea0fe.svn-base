package org.processmining.ericverbeek.plugins;

import org.deckfour.xes.model.XLog;
import org.processmining.contexts.uitopia.annotations.UITopiaVariant;
import org.processmining.ericverbeek.algorithms.XesCertificationFilterAlgorithm;
import org.processmining.framework.plugin.PluginContext;
import org.processmining.framework.plugin.annotations.Plugin;
import org.processmining.framework.plugin.annotations.PluginLevel;
import org.processmining.framework.plugin.annotations.PluginVariant;

@Plugin(name = "Filter log to XES X2 certification log", level = PluginLevel.Local, parameterLabels = { "Log" }, returnLabels = { "Filtered X2 log" }, returnTypes = { XLog.class }, userAccessible = true)
public class XesCertificationFilterX2Plugin extends XesCertificationFilterAlgorithm {

	@UITopiaVariant(affiliation = UITopiaVariant.EHV, author = "H.M.W Verbeek", email = "h.m.w.verbeek@tue.nl")
	@PluginVariant(requiredParameterLabels = { 0 })
	public XLog run(PluginContext context, XLog log) {
		return applyX2(log);
	}
}
